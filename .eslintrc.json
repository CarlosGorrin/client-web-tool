{
	"extends": "eslint:recommended",
	"rules": {
		"semi": ["error", "always"],
		"quotes": [0, "double"],
		"react/jsx-uses-react": "error",
		"react/jsx-uses-vars": "error",
		"no-console": "off"
	},
	"plugins": [
		"react"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true
	}
}